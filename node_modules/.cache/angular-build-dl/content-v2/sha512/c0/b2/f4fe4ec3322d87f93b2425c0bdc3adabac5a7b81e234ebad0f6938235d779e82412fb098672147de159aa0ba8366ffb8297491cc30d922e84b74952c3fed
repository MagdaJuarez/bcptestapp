        this.afDb = afDb;
        this.AGENCIES_COLLECTION = "agencies";
        this.isLoading$ = new rxjs__WEBPACK_IMPORTED_MODULE_3__["BehaviorSubject"](true);
    }
    get timestamp() {
        return new Date().getTime();
    }
    getAgenciesCollection() {
        return this.afDb.collection(this.AGENCIES_COLLECTION, ref => ref.orderBy("updated_at"));
    }
    getAgencies() {
        return this.getAgenciesCollection()
            .snapshotChanges()
